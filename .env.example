# Update these with your Supabase details from your project settings > API
# https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Get this from your project's API settings (service_role key)
SUPABASE_SERVICE_ROLE_KEY=

# Creem.io Payment Configuration
CREEM_WEBHOOK_SECRET=
CREEM_API_KEY=
CREEM_API_URL=https://test-api.creem.io/v1

# Site URLs
NEXT_PUBLIC_SITE_URL=http://localhost:3000
BASE_URL=localhost:3000

# Redirect success url after payment
CREEM_SUCCESS_URL=http://localhost:3000/dashboard

# AI Configuration for Pet Photo Generation
# Seedream 4 API Configuration for AI Image Generation
SEEDREAM_API_KEY=
SEEDREAM_API_URL=https://api.seedream.ai/v1
SEEDREAM_MODEL=seedream-4

# Image Storage Configuration
# For storing uploaded pet images and generated results
NEXT_PUBLIC_SUPABASE_STORAGE_BUCKET=pet-images

# Production Configuration (uncomment for production)
# CREEM_API_URL=https://api.creem.io
# NEXT_PUBLIC_SITE_URL=https://yourdomain.com
# CREEM_SUCCESS_URL=https://yourdomain.com/dashboard
